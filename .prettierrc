{
	"useTabs": true,
	"singleQuote": true,
	"bracketSameLine": true,
	"bracketSpacing": true,
	"svelteBracketNewLine": false,
	"singleAttributePerLine": true,
	"trailingComma": "es5",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
